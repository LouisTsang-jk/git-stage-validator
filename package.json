{
  "name": "commit-safe",
  "version": "1.0.0",
  "description": "",
  "type": "module",
  "main": "check.js",
  "scripts": {
    "pre-commit": "node --no-warnings --loader @swc-node/register/esm ./src/index.ts < /dev/tty",
    "dev": "node --loader @swc-node/register/esm ./src/index.old.ts < /dev/tty",
    "test": ". test/init.sh",
    "prepare": "husky install"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@inquirer/prompts": "^1.0.0",
    "@swc-node/register": "^1.6.5",
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.56",
    "@types/inquirer": "^9.0.3",
    "@types/jest": "^29.5.1",
    "chalk": "^5.2.0",
    "husky": "^8.0.0",
    "inquirer": "^9.1.5",
    "jest": "^29.5.0",
    "lint-staged": "^13.2.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*": "check.js"
  },
  "dependencies": {
    "swc-node": "^1.0.0"
  }
}
