{
  "name": "git-stage-validator",
  "version": "0.0.1",
  "description": "",
  "type": "module",
  "main": "lib/index.js",
  "bin": "lib/index.js",
  "scripts": {
    "build": "tsc",
    "rollup": "rollup -c",
    "next": "node --no-warnings --loader @swc-node/register/esm ./src/index.ts < /dev/tty",
    "legacy": "node --no-warnings --loader @swc-node/register/esm ./src/index.old.ts < /dev/tty",
    "dev": "node --loader @swc-node/register/esm ./src/index.old.ts < /dev/tty",
    "test": ". test/init.sh",
    "prepare": "husky install"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@inquirer/prompts": "^1.1.4",
    "@swc-node/register": "^1.6.5",
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.58",
    "@types/inquirer": "^9.0.3",
    "@types/jest": "^29.5.1",
    "chalk": "^5.2.0",
    "husky": "^8.0.3",
    "inquirer": "^9.2.4",
    "jest": "^29.5.0",
    "lint-staged": "^13.2.2",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*": "check.js"
  },
  "dependencies": {
    "@rollup/plugin-commonjs": "^25.0.0",
    "@rollup/plugin-node-resolve": "^15.0.2",
    "@rollup/plugin-swc": "^0.1.1",
    "rollup": "^3.22.0",
    "swc-node": "^1.0.0"
  }
}
